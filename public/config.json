{
	"modelPath": "snowman.glb",
	"connection": {
		"type": "influxdb",
		"org": "dev",
		"bucket": "dev",
		"token": "dev-token",
		"url": "http://localhost:8086"
	},
	"views": [
		{
			"display": "Thermal",
			"scene": {
				"material": "ghost"
			},
			"mapper": {
				"type": "thermal",
				"min": 0,
				"max": 100
			},
			"components": [
				{
					"id": "Body3",
					"indexerList": ["C"]
				},
				{
					"id": "Body2",
					"indexerList": ["B", "A"]
				},
				{
					"id": "Body1",
					"indexerList": ["C", "A"],
					"compute": "1 + 100 + 500 + 900"
				},
				{
					"id": "Nose",
					"isHidden": true
				}
			]
		},
    {
			"display": "Key Color",
			"mapper": {
				"type": "key-color",
				"map": {
					"hot": "#ff0000",
					"cold": "#0000ff",
					"ON": "#00ff00",
					"OFF": "#000000"
				}
			},
			"components": [
				{
					"id": "Body3",
					"compute": "'hot'",
					"material": "ghost"
				},
				{
					"id": "Body2",
					"compute": "'cold'"
				},
				{
					"id": "Body1",
          "indexerList": ["A"],
					"compute": "A % 2 === 0 ? 'ON' : 'OFF'"
				}
			]
		}
	]
}
