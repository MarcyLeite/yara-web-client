{
	"name": "web-client",
	"version": "0.6.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"build": "run-p type-check \"build-only {@}\" --",
		"build:demo": "vite build --mode=demo --base=/yara-web-client && cp dist/config-demo.json dist/config.json",
		"preview": "vite preview --host",
		"build-only": "vite build",
		"type-check": "vue-tsc --build",
		"lint": "eslint . --fix",
		"format": "prettier --write . && npm run lint",
		"test": "mocha",
		"analyze-release": "node ./scripts/analyze.mjs"
	},
	"dependencies": {
		"@influxdata/influxdb-client": "^1.35.0",
		"@mdi/js": "^7.4.47",
		"acorn": "^8.15.0",
		"axios": "^1.9.0",
		"pinia": "^3.0.1",
		"rand-seed": "^2.1.7",
		"three": "^0.177.0",
		"uuid-by-string": "^4.0.0",
		"vue": "^3.5.13"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.1",
		"@types/chai": "^5.2.2",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.14.0",
		"@types/sinon": "^17.0.4",
		"@types/three": "^0.176.0",
		"@vitejs/plugin-vue": "^5.2.3",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/eslint-config-typescript": "^14.5.0",
		"@vue/tsconfig": "^0.7.0",
		"chai": "^5.2.0",
		"eslint": "^9.22.0",
		"eslint-plugin-vue": "~10.0.0",
		"jiti": "^2.4.2",
		"mocha": "^11.5.0",
		"npm-run-all2": "^7.0.2",
		"prettier": "3.5.3",
		"sass": "^1.89.0",
		"semantic-release": "^24.2.5",
		"semantic-release-gitmoji": "^1.6.8",
		"sinon": "^20.0.0",
		"tsx": "^4.19.4",
		"typescript": "~5.8.0",
		"unplugin-auto-import": "^19.3.0",
		"unplugin-vue-components": "^28.7.0",
		"vite": "^6.2.4",
		"vite-plugin-vue-devtools": "^7.7.2",
		"vue-tsc": "^2.2.8"
	}
}
